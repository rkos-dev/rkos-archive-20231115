{
  "package_name": "tar",
  "package_version": "1.35",
  "package_url": "https://ftp.gnu.org/gnu/tar/tar-1.35.tar.xz",
  "depends": [
    "m4",
    "ncurses",
    "bash",
    "coreutils",
    "diffutils",
    "file",
    "findutils",
    "gawk",
    "grep",
    "gzip",
    "make",
    "patch",
    "sed"
  ],
  "rundeps": ["acl", "attr", "bzip2", "glibc", "gzip", "xz"],
  "testdeps": ["autoconf", "diffutils", "findutils", "gawk", "gzip"],
  "before": ["none"],
  "optdeps": ["none"],
  "script_path": "$PACKAGE_PATH/script/build.sh",
  "running_type": "sh",
  "envs": [{ "name": "PATH", "value": "$RKOS_TOOLCHAINS:$PATH" }]
}