{
  "package_name": "gettext",
  "package_version": "0.22",
  "package_url": "https://ftp.gnu.org/gnu/gettext/gettext-0.22.tar.xz",
  "depends": [
    "bash",
    "binutils",
    "coreutils",
    "gawk",
    "gcc",
    "glibc",
    "grep",
    "make",
    "ncurses",
    "sed"
  ],
  "rundeps": ["acl", "bash", "gcc", "glibc"],
  "testdeps": ["diffutils", "perl", "tcl"],
  "before": ["automake", "bison"],
  "optdeps": ["none"],
  "script_path": "$PACKAGE_PATH/script/build.sh",
  "running_type": "sh",
  "envs": [{ "name": "PATH", "value": "$RKOS_TOOLCHAINS:$PATH" }]
}